<!--school.wxml-->
<view class="search_input">
      <van-search shape="round" value="{{ value }}" placeholder="请输入搜索关键词" />
</view>
<view class="container top">
  <view style="width:100%;height:60%;">
    <image class="zheyeImg" src="../../images/zexiao.jpeg"/>
  </view>
  <view class="menu_Items">
      <view class="menu_Item" wx:for="{{icons}}" wx:key="id">
        <image class="menu_image" src="{{item.imgPath}}" />
        <view class="menu_text">{{item.label}}</view>
      </view>
    </view>
</view>
<view class="divider"></view>
<view class="container bottom">
  <van-tabs active="{{schooltype[0]}}">
  <van-tab wx:for="{{schooltype}}" wx:key="id" title="{{item}}" name="{{item}}">
    <view class='school_content'>
      <view class='school_item'>
        <view style="width:100%;height:70%;">
          <image class='school_image' src="../../images/logos/yeqiangwei.png" />
        </view>
        <view style="padding:10rpx 20rpx">
          <view class="school_name"></view>
          <view class="school_labels">
            <van-tag round plain size="medium" type="warning">985</van-tag>
            <van-tag round plain size="medium" type="warning">211</van-tag>
            <van-tag round plain size="medium" type="warning">国重点</van-tag>
          </view>
        </view>
      </view>
      <view class='school_item'></view>
      <view class='school_item'></view>
      <view class='school_item'></view>
    </view>
  </van-tab>
</van-tabs>
</view>
